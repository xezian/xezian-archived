type User {
  id: ID! @unique
  name: String! @unique
  password: String!
  secret: String!
}
type Project {
  id: ID! @unique
  title: String!
  description: String!
  link: String!
  repo: String
  img: String
  image: String
  img2: String
  image2: String
  img3: String
  image3: String
  sortorder: Int! @unique
}
type Guest {
  id: ID! @unique
  name: String!
  email: String!
  amount: Int!
  vegetarian: Int
  meat: Int
  note: String
  misc: String
}